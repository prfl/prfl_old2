@model Profile.Server.Pages.Products.DetailsModel

<!-- Website to visit when clicked in fb or WhatsApp-->
<meta property="fb:app_id"                  content="335372891152846" />
<meta property="og:url"                     content="https://prfl.ga/product/@Model.Product.ProductId" />
<meta property="og:type"                    content="product" />

<meta property="og:image"                   content="@Model.Product.ImageUrl" />
<meta property="og:type"                    content="website" />
<meta property="og:image:type"              content="image/jpeg">
<!-- Site Name, Title, and Description to be displayed -->
<meta property="og:site_name"               content="prfl">
<meta property="og:title"                   content="product">
<meta property="og:description"             content="@Model.Product.Description" />
<!-- Size of image. Any size up to 300. Anything above 300px will not work in WhatsApp -->
<meta property="og:image:width"             content="300">
<meta property="og:image:height"            content="300"> 
